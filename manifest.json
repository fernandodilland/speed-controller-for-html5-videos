{
    "name": "Speed Controller for HTML5 Videos",
    "version": "0.0.2",
    "description": "Speed configurator for HTML5 videos in web browsers.",
    "homepage_url": "https://github.com/fernandomireles/speed-controller-for-HTML5-videos/",
    "author": "Fernando Mireles <github.com/fernandomireles>",
    "web_accessible_resources": [
        "img/icon.png"
    ],
    "icons": {
        "16": "img/icon-16.png",
        "24": "img/icon-24.png",
        "32": "img/icon-32.png",
        "48": "img/icon-48.png",
        "64": "img/icon-64.png",
        "128": "img/icon-128.png",
        "256": "img/icon-256.png"
    },
    "browser_action": {
    "default_icon": "img/icon-256.png",
    "default_popup": "static/popup.html" 
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "css": [
                "css/main.css"
            ],
            "js": [
                "js/main.js"
            ]
        }
    ],
    "manifest_version": 2
}
